# Environment: production
# No secrets should be committed. Copy to a real env file and inject at runtime.

ENVIRONMENT=production

# Logging
LOG_LEVEL=INFO

# Optional: request correlation / tracing (populated by upstream proxy or middleware when available)
TRACE_ID=

# Postgres
DATABASE_URL=
DATABASE_DSN=

# Object storage (S3)
S3_ENDPOINT_URL=
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_BUCKET=payscope-raw

# Redis / Celery
CELERY_BROKER_URL=

# Optional: HF model cache
HF_HOME=

# LLM mapping (OpenAI-compatible)
LLM_BASE_URL=
LLM_API_KEY=
LLM_MODEL=
LLM_TIMEOUT_S=30
MAPPING_CONFIDENCE_THRESHOLD=0.70

# Pinecone (vector DB)
PINECONE_API_KEY=
PINECONE_INDEX_NAME=
PINECONE_NAMESPACE=

# Neo4j (graph DB)
NEO4J_URI=
NEO4J_USER=
NEO4J_PASSWORD=


